    "stream-buffers": "^3.0.1",
    "tmp": "0.0.33",
    "web3": "1.2.1",
    "webpack": "^2.5.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "yargs": "^8.0.2"
  },
  "publishConfig": {
    "access": "public"
